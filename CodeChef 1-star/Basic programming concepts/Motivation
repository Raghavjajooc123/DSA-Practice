#include <iostream>
using namespace std;

int main() {
	// Motivation
	//Contest Code:LP1TO201 Problem Code:IMDB
	int t;
	cin>>t;
	while(t--){
	    int n,x;
	    cin>>n>>x;
	    int ans = 0;
	    for(int i=0; i<n; i++){
	        int s,r;
	        cin>>s>>r;
	        if(s<=x){
	            ans = max(ans, r);
	        }
	    }
	    cout<<ans<<endl;
	}
	return 0;
}
