#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    for(int i=0; i<t; i++){
        int a;
        cin>>a;
        int ans = 0;
        for(int j=0; a!=0; j++){
            int digit = a%10;
            ans = ans*10 + digit;
            a = a/10;
        }
        cout<<ans<<endl;
    }

    return 0;
}
