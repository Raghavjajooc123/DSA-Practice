#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n, k;
    cin>>n>>k;
    int count = 0;
    for(int i=0; i<n; i++){
        int temp;
        cin>>temp;
        if(temp%k==0){
            count++;
        }
    }
    cout<<count<<endl;
    return 0;
}
