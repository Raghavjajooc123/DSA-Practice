#include<bits/stdc++.h>
using namespace std;

int main()
{
   
    int t;
    cin>>t;
    for(int i=0;i<t; i++){
        int a;
        cin>>a;
        int ans = 0;
        ans = ans + a%10;
        while(a>=10){
            a = a/10;
        }
       ans = ans+ a;
       cout<<ans<<endl;
   }
return 0;

}


