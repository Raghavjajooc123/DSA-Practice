#include <iostream>
using namespace std;

int main() {
	// reading a list of strings from the user and saving in a char array
	int n;
	cout<<"enter the number of strings "<<endl;
	cin>>n;
	cin.get();
	
	char arr[n][100];
	cout<<"Enter the strings "<<endl;
	
	for(int i=0; i<n; i++){
	    cin.getline(arr[i], 100);
	}
	
	for(int i=0; i<n; i++){
	    cout<<arr[i]<<endl;
	}
	return 0;
}
