#include <iostream>
using namespace std;

int main() {
	// Method 3 - Sum of submatrices
	int m,n;
	int sum=0;
	cout<<"Enter the rows and cols "<<endl;
	cin>>m>>n;
	
	int arr[m][n];
	cout<<"enter the array "<<endl;
	for(int i=0; i<m; i++){
	    for(int j=0; j<n; j++){
	        cin>>arr[i][j];
	    }
	}
	
	for(int i=0; i<m; i++){
	    for(int j=0; j<n; j++){
	        int countTL=0;
            int countBR=0;
	        //Number of toplefts
	        for(int a=0; a<=i; a++){
	            for(int b=0; b<=j; b++){
	                countTL++;
	            }
	        }
	        //Number of Bottomrights
	        for(int a=i; a<m; a++){
	            for(int b=j; b<n; b++){
	                countBR++;
	            }
	        }
	        
	        //the no of submatrices in which theelement is present would be countBR*countTL
	        //the sum calculation
	        sum = sum + (arr[i][j]*(countBR*countTL));
	    }
	}
	
	cout<<"Sum is "<<sum;
    
	return 0;
}
