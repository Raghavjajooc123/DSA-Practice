#include <iostream>
using namespace std;

int main() {
	// Vaccine Dates
	// Contest Code:LP1TO201 Problem Code:VDATES
	int t;
	cin>>t;
	while(t--){
	    int d,r,l;
	    cin>>d>>r>>l;
	    if(d<r){
	        cout<<"Too Early"<<endl;
	    }
	    else if(d>=r && d<=l){
	        cout<<"Take second dose now"<<endl;
	    }
	    else{
	        cout<<"Too Late"<<endl;
	    }
	}
	return 0;
}
