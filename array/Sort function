#include <iostream>
#include<algorithm>
using namespace std;

int main() {
    //using sort function
    
	int arr[100];
    int n, key;
    
    cout<<"Enter the number of elements in the array "<<endl;
    cin>>n;
    
    cout<<"enter the elements of the array "<<endl;
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    
    sort(arr, arr+n);
    
    for(int i=0; i<n;i++){
        cout<<arr[i]<<" ";
    }
	return 0;
}
